{
  "name": "react-native-website-monorepo",
  "private": true,
  "workspaces": [
    "website",
    "plugins/*",
    "sync-api-docs"
  ],
  "scripts": {
    "postinstall": "yarn update-lock",
    "update-lock": "npx yarn-deduplicate"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  },
  "dependencies": {
    "husky": "^4.3.8",
    "prettier": "^2.2.1",
    "pretty-quick": "^3.1.0"
  }
}
